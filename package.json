{
  "name": "website-next",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "dev:website": "turbo run dev -F @website-next/website",
    "build:website": "turbo run build -F @website-next/website",
    "start:website": "turbo run start -F @website-next/website",
    "dev:docs": "turbo run dev -F @website-next/documentation",
    "build:docs": "turbo run build -F @website-next/documentation",
    "start:docs": "turbo run start -F @website-next/documentation",
    "dev:console": "turbo run dev -F @website-next/console",
    "build:console": "turbo run build -F @website-next/console",
    "start:console": "turbo run start -F @website-next/console",
    "dev:web": "turbo run dev -F @website-next/console -F @website-next/website",
    "build:web": "turbo run build -F @website-next/console @website-next/website",
    "start:web": "turbo run start -F @website-next/console @website-next/website",
    "clean:logs": "bash scripts/clean_logs.sh",
    "lint": "next lint"
  },
  "dependencies": {
    "@ricons/tabler": "^0.12.0",
    "@ricons/utils": "^0.1.6",
    "@tabler/icons-react": "^3.19.0",
    "next": "16.1.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "postcss": "^8.5.3",
    "turbo": "^2.7.4",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
