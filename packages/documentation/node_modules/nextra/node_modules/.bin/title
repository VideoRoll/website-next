#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/programs/website-next/node_modules/.pnpm/title@4.0.1/node_modules/title/dist/esm/node_modules:/mnt/c/programs/website-next/node_modules/.pnpm/title@4.0.1/node_modules/title/dist/node_modules:/mnt/c/programs/website-next/node_modules/.pnpm/title@4.0.1/node_modules/title/node_modules:/mnt/c/programs/website-next/node_modules/.pnpm/title@4.0.1/node_modules:/mnt/c/programs/website-next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/programs/website-next/node_modules/.pnpm/title@4.0.1/node_modules/title/dist/esm/node_modules:/mnt/c/programs/website-next/node_modules/.pnpm/title@4.0.1/node_modules/title/dist/node_modules:/mnt/c/programs/website-next/node_modules/.pnpm/title@4.0.1/node_modules/title/node_modules:/mnt/c/programs/website-next/node_modules/.pnpm/title@4.0.1/node_modules:/mnt/c/programs/website-next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../title@4.0.1/node_modules/title/dist/esm/bin.js" "$@"
else
  exec node  "$basedir/../../../../../title@4.0.1/node_modules/title/dist/esm/bin.js" "$@"
fi
